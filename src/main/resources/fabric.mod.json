{
  "schemaVersion": 1,
  "id": "betterblockoutline",
  "version": "${version}",
  "name": "BetterBlockOutline",
  "description": "A client sided Minecraft mod that allows for ample customization of the block outline. Everything from color customization to debug information.",
  "authors": [
    "DarkKronicle"
  ],
  "contact": {},
  "license": "MPL-2.0",
  "icon": "assets/betterblockoutline/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.github.darkkronicle.betterblockoutline.BetterBlockOutline"
    ],
    "modmenu": [
      "io.github.darkkronicle.betterblockoutline.config.ModMenuImpl"
    ]
  },
  "mixins": [
    "betterblockoutline.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.12",
    "minecraft": ">=1.19"
  }
}
